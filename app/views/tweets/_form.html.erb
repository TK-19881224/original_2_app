<%= form_with(model: @tweet, local: true) do |form| %>
     <%= form.label :image, "画像" %><br />
     <%= form.file_field :image %>
      <p><%= form.text_field :title, placeholder: "タイトル" %></p>
      <p><%= form.text_area :text, placeholder: "内容", rows: "3" %></p>
      <p><%= form.submit "SEND" %></p>
    <% end %>